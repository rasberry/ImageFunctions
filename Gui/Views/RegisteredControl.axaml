<UserControl
	xmlns="https://github.com/avaloniaui"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:vm="using:ImageFunctions.Gui.ViewModels"
	x:Class="ImageFunctions.Gui.Views.RegisteredControl"
	x:DataType="vm:SelectionViewModel"
	d:DesignHeight="450"
	d:DesignWidth="800"
	mc:Ignorable="d"
>
<Design.DataContext>
	<vm:SelectionViewModel/>
</Design.DataContext>

<Expander Header="{Binding Kind}">
	<ListBox ItemsSource="{Binding Items}" SelectionChanged="OnItemSelected" />
</Expander>

</UserControl>